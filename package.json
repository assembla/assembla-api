{
  "name": "assembla-api",
  "version": "0.0.5",
  "description": "API client for Assembla",
  "author": "Assembla Inc.",
  "main": "dist/assembla.js",
  "scripts": {
    "lint": "eslint {src,spec}/*{,/*}.js",
    "test": "mocha --compilers js:babel-core/register --recursive spec/*{,/*}_spec.js",
    "build": "rm -rf ./dist/*; npm run lint; npm run test; node_modules/.bin/babel src -d dist"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/assembla/assembla-api.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "assembla",
    "api"
  ],
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "eslint": "^4.5.0",
    "mocha": "^3.5.0",
    "chai": "^4.1.2"
  },
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/assembla/assembla-api/issues"
  },
  "homepage": "https://github.com/assembla/assembla-api#readme"
}
